<?xml version="1.0" encoding="utf-8"?>
<Product>
	<Model>NAS-WR01Z</Model>
	<Label lang="en">Smart Power Plug</Label>
	<CommandClasses>
        <!-- NO_OPERATION -->
		<Class><id>0x00</id></Class>
        <!-- BASIC -->
		<Class><id>0x20</id></Class>
        <!-- SWITCH_BINARY -->
		<Class><id>0x25</id></Class>
        <!-- SWITCH_ALL -->
		<Class><id>0x27</id></Class>
        <!-- METER -->
		<Class><id>0x32</id></Class>
        <!-- ASSOCIATION_GROUP_INFO -->
        <Class><id>0x59</id></Class>
        <!-- DEVICE_RESET_LOCALLY -->
        <Class><id>0x5a</id></Class>
        <!-- ZWAVE_PLUS_INFO -->
        <Class><id>0x5e</id></Class>
        <!-- CONFIGURATION -->
		<Class><id>0x70</id></Class>
        <!-- ALARM -->
		<Class><id>0x71</id></Class>
        <!-- MANUFACTURER_SPECIFIC -->
		<Class><id>0x72</id></Class>
        <!-- POWERLEVEL -->
        <Class><id>0x73</id></Class>
        <!-- ASSOCIATION -->
		<Class><id>0x85</id></Class>
        <!-- VERSION -->
		<Class><id>0x86</id></Class>
	</CommandClasses>
	<Configuration>
		<Parameter>
			<Index>1</Index>
			<Type>list</Type>
			<Default>1</Default>
			<Size>1</Size>
			<Label lang="en">Send meter report</Label>
			<Item>
				<Value>0</Value>
				<Label lang="en">disable</Label>
			</Item>
			<Item>
				<Value>1</Value>
				<Label lang="en">enable</Label>
			</Item>
			<Help lang="en">Send meter report</Help>
		</Parameter>

		<Parameter>
			<Index>2</Index>
			<Type>byte</Type>
			<Default>300</Default>
			<Size>2</Size>
			<Label lang="en">Meter report interval</Label>			
			<Help lang="en">Meter report interval</Help>
		</Parameter>

		<Parameter>
			<Index>3</Index>
			<Type>byte</Type>
			<Default>12</Default>
			<Size>1</Size>
			<Label lang="en">Maximum over-load current</Label>			
			<Help lang="en">This parameter defines maximum current the plug can provide to load that be connected to plug.
			If the current consumed by load greater than maximum current, the plug will cut off power. 
			</Help>
		</Parameter>

		<Parameter>
            <Index>4</Index>
            <Type>byte</Type>
            <Default>12</Default>
            <Size>1</Size>
            <Label lang="en">Maximum alarm current</Label>          
            <Help lang="en">This parameter defines maximum current, if the current plug provide to load is greater 
            than this parameter, the plug will send over-current notification to the controller without cutting off power. 
            This value must be less than parameter #3.
            </Help>
		</Parameter>

        <Parameter>
            <Index>5</Index>
            <Type>list</Type>
            <Default>1</Default>
            <Size>1</Size>
            <Label lang="en">Led display</Label>
            <Item>
                <Value>0</Value>
                <Label lang="en">disable</Label>
            </Item>
            <Item>
                <Value>1</Value>
                <Label lang="en">enable</Label>
            </Item>
            <Help lang="en">Led display enable</Help>
        </Parameter>

        <Parameter>
            <Index>6</Index>
            <Type>byte</Type>
            <Default>30</Default>
            <Size>1</Size>
            <Label lang="en">Power report threshold</Label>
            <Help lang="en">Power report threshold</Help>
        </Parameter>

        <Parameter>
            <Index>7</Index>
            <Type>list</Type>
            <Default>255</Default>
            <Size>1</Size>
            <Label lang="en">Remember relay state after power failure</Label>
            <Item>
                <Value>0</Value>
                <Label lang="en">disable</Label>
            </Item>
            <Item>
                <Value>1</Value>
                <Label lang="en">enable</Label>
            </Item>
            <Help lang="en">If disabled the pulg will stay OFF after power failure. When enabled it will restore previous state.
            </Help>
        </Parameter>

        <Parameter>
            <Index>8</Index>
            <Type>list</Type>
            <Default>0</Default>
            <Size>1</Size>
            <Label lang="en">Switch OFF timer</Label>
            <Item>
                <Value>0</Value>
                <Label lang="en">disable</Label>
            </Item>
            <Item>
                <Value>1</Value>
                <Label lang="en">enable</Label>
            </Item>
            <Help lang="en">When enabled after turning ON the timer starts and after time defined in #9 it will turn power OFF.
            </Help>
        </Parameter>

        <Parameter>
            <Index>9</Index>
            <Type>byte</Type>
            <Default>150</Default>
            <Size>2</Size>
            <Label lang="en">Switch OFF time</Label>
            <Help lang="en">This parameter defines time in minutes for switch OFF timer. The value can be set to 1-65535 minutes.
            </Help>
        </Parameter>

	</Configuration>

	<Associations>
		<Group>
			<Index>1</Index>
			<Maximum>1</Maximum>
			<Label lang="en">Lifeline to controller</Label>
			<Help lang="en">Lifeline service that assigned to plug status – ON/OFF. It enables the Plug to send 
            reports and readings to Z‐Wave Controller or Z‐Wave Gateway. This Group Support: NOTIFICATION_REPORT_V4 
            METER_REPORT_V4 SWITCH_BINARY_REPORT DEVICE_RESET_LOCALLY_NOTIFICATION</Help>
			<SetToController>true</SetToController>
		</Group>
		<Group>
			<Index>2</Index>
			<Maximum>5</Maximum>
			<Label lang="en">Basic on/off</Label>
			<Help lang="en">Allows for sending control commands to associated devices such as Siren, relay module, lighting, 
            etc. If current load is over the max current defined in parameter #3, the Plug will send a BASIC_SET (0xFF) 
            to associated devices. When current load is normal, plug will send a BASIC_SET (0x00) to associated devices. 
            This Group Support: BASIC_SET</Help>
		</Group>
		<Group>
			<Index>3</Index>
			<Maximum>5</Maximum>
			<Label lang="en">Notification report</Label>
			<Help lang="en">Allows for Send Notification to associated devices in this group. 
            This Group Support: NOTIFICATION_REPORT_V4</Help>
		</Group>
	</Associations>
</Product>

